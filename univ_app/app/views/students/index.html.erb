<div class="row">
  <div class="col">
    <h3 class="text-center">
      Student Listing
    </h3>
  </div>
</div>
<div class="container my-4">
  <div class="row">
    <% @students.each do |student| %>
      <div class="col-6 mb-4">
        <div class="card text-white bg-secondary">
          <div class="card-body">
            <h5 class="card-title">Name: <%= student.name %></h5>
            <h6 class="card-subtitle mb-1 text-white">Email: <%= student.email %></h6>
            <p class="card-text">Course Enrollments: None</p>
          </div>
          <div class="card-footer text-muted">
            <a href="#" class="card-link text-info">View Profile</a>
          </div>
        </div>
      </div>
    <% end %>
  </div>
</div>